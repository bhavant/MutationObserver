{
  "manifest_version": 2,
  "name": "Page Load Detector",
  "description": "This extension checks if DOM changes are done.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "notifications",
    "http://*/",
    "https://*/"
  ]
}